pipeline {
    agent any
    environment {
        MANI_SSHKEYY = credentials('MANISHA-SSH')
    }

    stages{
        stage('Accessing Credentials'){
             steps{
                sh ('echo username for SSH  is: ${MANI_SSHKEYY_USR}')
                sh ('echo my-ssh-key is: ${MANI_SSHKEYY_PSW}')
             }
             
             
        }

    }
}